{
    "name": "gowebpro/infoflot",
    "type": "library",
    "description": "Infoflot API for Laravel",
    "keywords": [
        "gowebpro",
        "laravel",
        "infoflot api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Chiffa",
            "email": "i@ars-team.ru",
            "homepage": "http://goweb.pro"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "Gowebpro\\Infoflot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gowebpro\\Infoflot\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gowebpro\\Infoflot\\InfoflotServiceProvider"
            ],
            "aliases": {
                "Infoflot": "Gowebpro\\Infoflot\\InfoflotFacade"
            }
        }
    }
}
